{
  "name": "pawpal_backend",
  "version": "1.0.0",
  "description": "PawPal is a platform designed for dog owners to connect, share experiences, find dog training tips, playdates, and product recommendations.",
  "license": "ISC",
  "author": "Maya Klein & Linoy Eli",
  "type": "commonjs",
  "main": "app.js",
  "scripts": {
    "start": "tsc && node ./dist/src/app.js",
    "dev": "nodemon ./src/app.ts",
    "test": "tsc && jest --detectOpenHandles --forceExit --coverage --runInBand",
    "testAuth": "tsc && jest --detectOpenHandles --forceExit auth.test.ts",
    "testPost": "tsc && jest --detectOpenHandles --forceExit post.test.ts",
    "testComment": "tsc && jest --detectOpenHandles --forceExit comment.test.ts",
    "testUser": "tsc && jest --detectOpenHandles --forceExit user.test.ts",
    "testPlaydate": "tsc && jest --detectOpenHandles --forceExit playdate.test.ts",
    "testFile": "tsc && jest --detectOpenHandles --forceExit file.test.ts",
    "testAI": "tsc && jest --detectOpenHandles --forceExit ai.test.ts",
    "testGoogle": "tsc && jest --detectOpenHandles --forceExit google.test.ts",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/supertest": "^6.0.2",
